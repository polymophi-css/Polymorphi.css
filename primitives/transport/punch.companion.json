{
  "name": "punch",
  "version": "1.0.0",
  "category": "transport",
  "description": "Punch in/out recording",
  "bindings": {
    "color": "--pm-{color}-base-{index}",
    "punchInColor": "--pm-red-base-{index}",
    "punchOutColor": "--pm-red-shade-{index}",
    "regionColor": "--pm-red-alpha-{index}"
  },
  "behaviors": {
    "mode": [
      "manual",
      "auto"
    ],
    "region": "draggable"
  },
  "constraints": {
    "indexRange": "000-255"
  },
  "accessibility": {
    "role": "group"
  },
  "relationships": [
    "record",
    "transport",
    "arrange"
  ]
}